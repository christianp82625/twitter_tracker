<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
<title>Login</title>
<link type="text/css" href="/css/jquery.jscrollpane.css" rel="stylesheet" media="all" />
<link href="/css/style.css" rel="stylesheet" type="text/css" />
<link href="/css/bootstrap.css" rel="stylesheet" type="text/css" />
<link href="/css/jquery.mCustomScrollbar.css" rel="stylesheet" type="text/css" />
<link href="/css/bootstrap-responsive.css" rel="stylesheet" type="text/css" />
<link href="/css/chosen.css" rel="stylesheet" type="text/css" />
<link href="/css/jquery.jgrowl.css" rel="stylesheet" type="text/css" />
<%- renderCssTags(styles) %>

<script type="text/javascript" src="http://ajax.googleapis.com/ajax/libs/jquery/1/jquery.min.js"></script>

<script type="text/JavaScript" src="js/bootstrap.js"></script>
<script type="text/JavaScript" src="js/ejs_0.9_alpha_1_production.js"></script>
<link href='http://fonts.googleapis.com/css?family=Open+Sans:400,300,600,700' rel='stylesheet' type='text/css'>
<script type="text/javascript" src="/socket.io/socket.io.js"></script> 

<script src="/zoom_assets/jquery.smoothZoom.min.js"></script>		
<link rel="stylesheet" type="text/css" href="/css/sentiment.css"/>
<link rel="stylesheet" type="text/css" href="/css/dark-hive/jquery-ui-1.8.22.custom.css"/>

<script src="/js/date.format.js" type="text/javascript"></script>
<script src="/js/main.js" type="text/javascript"></script>
<script src="/js/bridge/FABridge.js"></script>
<script src="/js/twitterfeed.js"></script>
<script src="/js/twitterlib.js"></script>

<!--<script type="text/javascript" src="scripts/jquery-1.2.6.min.js"></script>-->

<%- renderScriptsTags(scripts) %>
</head>

<body>
	<div id="notif-container" style="display:none">
		<div id="basic-template">
			<a class="ui-notify-cross ui-notify-close" href="#">x</a>
			<h1>#{title}</h1>
			<p>#{text}</p>
		</div>
	
	</div>

<!----------header------------>

<div id="loader-gif">
	<img src="/img/loader.gif" /><br />
    Please wait...
</div>
<div class="bg-wrapper">
	<div class="header-wrapper">
        <% if(title!="LOGIN") { %>
		<div class="container header">
			<h1><a href="/" title="Twitter Tracker" target="_blank"></a></h1>
			<div class="header-title">
				<h2><%= title %></h2>
			</div>
			<% if(title=="MAIN DASHBOARD" || title=="SEARCH") { %>
			<div class="header-info">
				<h3>Keywords:<span id="total-keywords-span">...</span></h3>
				<h3>Total Tweets:<span id="total-tweets-span">...</span> </h3>
			</div>
			<% } %>
			<a href="/search" <% if(title!="SEARCH") { %>class="header-search"<% } else { %>class="header-search active"<% } %> target="_blank">Advanced Search</a><a href="/keywords" <% if(title!="KEYWORDS") { %>class="header-search special"<% } else { %>class="header-search special active"<% } %> target="_blank">Keywords</a><a href="/influencers" <% if(title!="INFLUENCERS") { %>class="header-search special"<% } else { %>class="header-search special active"<% } %> target="_blank">Influencers</a>
            <ul class="header-nav hide">
                <li class="admin"><a <% if(title!="ADMIN PANEL") { %>target="_blank" <% } %> href="/admin">Admin </a></li>
                <li class="admin last"><a href="/logout">Logout </a></li>
            </ul>
            <ul class="nav nav-pills header-nav hide">
                <li class="dropdown">
                    <a href="#" data-toggle="dropdown" class="dropdown-toggle"><img src="img/system.png" /><b class="caret"></b></a>
                    <ul class="dropdown-menu">
                        <li class="admin"><a <% if(title!="ADMIN PANEL") { %>target="_blank" <% } %>  href="/admin">Admin </a></li>
                        <li class="admin last"><a href="/logout">Logout </a></li>
                    </ul>
                </li>
            </ul>
		</div>
        <% } %>
	</div>
	<%- body %>
</div>
<script src="http://ajax.googleapis.com/ajax/libs/jquery/1.7/jquery.min.js"></script> 
<script src="/jquery/jquery-ui-1.8.22.custom.min.js"></script> 
<script src="/js/jquery.mousewheel.min.js"></script> 
<script>!window.jQuery.ui && document.write(unescape('%3Cscript src="jquery/jquery-ui-1.8.21.custom.min.js"%3E%3C/script%3E'))</script> 
<script src="js/jquery.mCustomScrollbar.js"></script> 

<script src="/js/customScrollbar.js"></script>
<script src="/js/jquery.timeago.js"></script>
<script src="/js/jquery.easydate-0.2.4.min.js"></script>
<script src="/js/chosen.jquery.min.js"></script>
<!--<script src="/js/jquery.mousewheel.js"></script>
<script src="/js/jScrollPane.js"></script>-->

<script type="text/javascript" src="/js/jquery.mousewheel.js"></script>
<script type="text/javascript" src="/js/jquery.jscrollpane.min.js"></script>
<script src="/js/jquery.jgrowl.js"></script>
<% if(title=="MAIN DASHBOARD") { %>
<script type="text/javascript" charset="utf-8" src="/js/heatmap.js"></script> 
<script type="text/javascript" charset="utf-8" src="/js/sentiment.js"></script>
<% } %>

</body>
</html>